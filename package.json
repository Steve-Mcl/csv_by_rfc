{
  "name": "csv_4180",
  "version": "0.2.0",
  "description": "CSV according to RFC 4180.  Everyone else's are weirdly wrong.  Heavily tested; in typescript",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf -f build",
    "typescript": "tsc",
    "nyc": "nyc ava build/tests/*.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "npm run nyc",
    "build": "npm run clean && npm run typescript && npm run test",
    "travis": "npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StoneCypher/csv_4180.git"
  },
  "keywords": [
    "csv",
    "4180",
    "rfc4180",
    "rfc-4180",
    "tsv",
    "comma",
    "separated",
    "values",
    "seperated",
    "tab",
    "comma-separated-values"
  ],
  "author": "John Haugeland <stonecypher@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StoneCypher/csv_4180/issues"
  },
  "homepage": "https://github.com/StoneCypher/csv_4180#readme",
  "devDependencies": {
    "ava": "^0.24.0",
    "ava-spec": "^1.1.1",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "typescript": "^2.6.2"
  }
}
